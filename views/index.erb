<h2>All Expenses</h2>
<table>
  <tr>
    <th>Date</th>
    <th>Category</th>
    <th>Amount</th>
    <th>Description</th>
  </tr>
  <% @expenses.each do |e| %>
    <tr>
      <td><%= e.date %></td>
      <td><%= e.category %></td>
      <td>$<%= e.amount %></td>
      <td><%= e.description %></td>
    </tr>
  <% end %>
</table>
